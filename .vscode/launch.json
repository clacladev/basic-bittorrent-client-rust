  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'bittorrent-starter-rust'",
      "cargo": {
        "args": [
          "build",
          "--bin=bittorrent-starter-rust",
          "--package=bittorrent-starter-rust"
        ],
        "filter": {
          "name": "bittorrent-starter-rust",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'bittorrent-starter-rust'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=bittorrent-starter-rust",
          "--package=bittorrent-starter-rust"
        ],
        "filter": {
          "name": "bittorrent-starter-rust",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug './your_bittorrent.sh download_piece -o /tmp/test-piece-0 sample.torrent 0'",
      "cargo": {
        "args": [
          "build",
          "--bin=bittorrent-starter-rust",
          "--package=bittorrent-starter-rust"
        ],
        "filter": {
          "name": "bittorrent-starter-rust",
          "kind": "bin"
        }
      },
      "args": [
        "download_piece",
        "-o",
        "/tmp/test-piece-0",
        "sample.torrent",
        "0"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
